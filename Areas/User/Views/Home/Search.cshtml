@model List<AustraliaSays2_Models.DTO.SearchResult>
@{
    ViewData["Title"] = "Search";
}

<h1>Search Results</h1>

@if (Model == null || !Model.Any())
{
    <p>No results found.</p>
}
else
{
    <ul>
        @foreach (var result in Model)
        {
            <li>
                <a href="@result.Url">@result.Name</a> <span>- @result.Type</span>
            </li>
        }
    </ul>
}
